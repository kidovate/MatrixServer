<Window x:Class="MMOTestClient.LoginForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoginForm" Height="180" Width="226" ResizeMode="NoResize">
    <Grid HorizontalAlignment="Left" Width="226" Margin="0,0,-6,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Content="Log In" HorizontalAlignment="Left" Margin="85,0,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14" Height="29" Width="51"/>
        <Label Content="Username: " HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.053,0" Height="26" Width="69"/>
        <Label Content="Password: " HorizontalAlignment="Left" Margin="14,69,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.053,0" Height="26" Width="66"/>
        <TextBox x:Name="usernameBox" HorizontalAlignment="Left" Height="23" Margin="84,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Left" Height="23" Margin="85,72,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="loginButton" Content="Log In" HorizontalAlignment="Left" Margin="75,111,0,0" VerticalAlignment="Top" Width="75" Click="loginButton_Click"/>

    </Grid>
</Window>
